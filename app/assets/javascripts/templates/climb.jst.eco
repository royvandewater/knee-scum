<% if @model: %>
  <h1>
    <%= @model.name %>
    <span class="badge"><%= @model.difficulty %></span>
    <small><a class="edit" href="<%= @edit_url %>">Edit</a></small>
  </h1>

  <p><%= @model.description %></p>

  <div class="container panel photos-container">
  </div>
<% else: %>
  <h1>&nbsp;</h1>
  <div class="alert alert-danger">
    <strong>Error:</strong>
    Climb was not found
  </div>
<% end %>
